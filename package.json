{
  "name": "coding-ui",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev:api": "node api-server.js",
    "dev:full": "concurrently \"npm run dev:api\" \"npm run dev\"",
    "build": "tsc -b && vite build",
    "build:analyze": "ANALYZE=true npm run build",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write \"src/**/*.{ts,tsx,css,json,md}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,css,json,md}\"",
    "type-check": "tsc --noEmit",
    "validate": "npm run type-check && npm run lint && npm run format:check && npm run test:run",
    "preview": "vite preview",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "test:run": "vitest run",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:record": "playwright codegen http://localhost:5173",
    "test:e2e:report": "playwright show-report",
    "test:e2e:debug": "playwright test --debug",
    "test:all": "npm run test:run && npm run test:e2e",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "deploy": "./deploy.sh",
    "deploy:win": "deploy.bat",
    "deploy:advanced": "./deploy-advanced.sh",
    "deploy:dry": "./deploy-advanced.sh -d",
    "deploy:help": "./deploy-advanced.sh -h",
    "quick": "./quick-deploy.sh"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.67.0",
    "@google/generative-ai": "^0.24.1",
    "@headlessui/react": "^2.2.9",
    "@sentry/react": "^10.17.0",
    "@sentry/vite-plugin": "^4.3.0",
    "@supabase/supabase-js": "^2.58.0",
    "@tanstack/react-query": "^5.90.2",
    "@tanstack/react-query-devtools": "^5.90.2",
    "@types/crypto-js": "^4.2.2",
    "@types/react-window": "^1.8.8",
    "axios": "^1.6.5",
    "bull": "^4.12.0",
    "clsx": "^2.1.1",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "crypto-js": "^4.2.0",
    "csrf-csrf": "^4.0.3",
    "exceljs": "^4.4.0",
    "express": "^4.18.2",
    "express-rate-limit": "^8.1.0",
    "file-type": "^21.0.0",
    "focus-trap-react": "^11.0.4",
    "helmet": "^8.1.0",
    "idb": "^8.0.3",
    "ioredis": "^5.3.2",
    "isomorphic-dompurify": "^2.28.0",
    "lucide-react": "^0.544.0",
    "multer": "^2.0.2",
    "openai": "^6.2.0",
    "papaparse": "^5.5.3",
    "react": "^19.1.1",
    "react-arborist": "^3.4.0",
    "react-dnd": "^16.0.1",
    "react-dnd-html5-backend": "^16.0.1",
    "react-dom": "^19.1.1",
    "react-router-dom": "^7.9.3",
    "react-virtualized-auto-sizer": "^1.0.26",
    "react-window": "^1.8.10",
    "recharts": "^3.2.1",
    "sonner": "^2.0.7",
    "tailwind-merge": "^3.3.1",
    "xlsx": "^0.18.5",
    "zod": "^4.1.12"
  },
  "overrides": {
    "@testing-library/react": {
      "react": "^19.0.0",
      "react-dom": "^19.0.0"
    },
    "openai": {
      "zod": "$zod"
    }
  },
  "devDependencies": {
    "@eslint/js": "^9.36.0",
    "@playwright/test": "^1.40.0",
    "@tailwindcss/postcss": "^4.1.14",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/node": "^24.6.0",
    "@types/react": "^19.1.16",
    "@types/react-dom": "^19.1.9",
    "@vitejs/plugin-react": "^5.0.4",
    "@vitest/coverage-v8": "^3.2.4",
    "@vitest/ui": "^3.2.4",
    "autoprefixer": "^10.4.21",
    "concurrently": "^8.2.2",
    "eslint": "^9.36.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.22",
    "globals": "^16.4.0",
    "happy-dom": "^20.0.5",
    "jsdom": "^23.2.0",
    "msw": "^2.0.0",
    "postcss": "^8.5.6",
    "prettier": "^3.6.2",
    "rollup-plugin-visualizer": "^6.0.4",
    "tailwindcss": "^4.1.14",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.45.0",
    "vite": "^7.1.7",
    "vite-plugin-compression": "^0.5.1",
    "vitest": "^3.2.4"
  }
}
